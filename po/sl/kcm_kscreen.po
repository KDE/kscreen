#, kde-format
msgctxt "Global scale factor expressed in percentage form"
msgid "%1%"
msgstr "%1 %"

#: ui/OutputPanel.qml:133
#, kde-format
msgid "Refresh rate:"
msgstr "Hitrost osveževanja:"

#: ui/OutputPanel.qml:154
#, kde-format
msgctxt "@info"
msgid "“%1” is the only refresh rate supported by this display."
msgstr "“%1” je edina hitrost osveževanja, ki jo podpira ta zaslon."

#: ui/OutputPanel.qml:159
#, kde-format
msgid "Adaptive sync:"
msgstr "Prilagodljivo usklajevanje:"

#: ui/OutputPanel.qml:162
#, kde-format
msgid "Never"
msgstr "Nikoli"

#: ui/OutputPanel.qml:164
#, kde-format
msgid "Always"
msgstr "Vedno"

