msgid "in percent of regular scale"
msgstr "v odstotku regularne lestvice"

#: ui/OutputPanel.qml:119 ui/Panel.qml:113
#, kde-format
msgctxt "Global scale factor expressed in percentage form"
msgid "%1%"
msgstr "%1 %"

#: ui/OutputPanel.qml:131
#, kde-format
msgid "Refresh rate:"
msgstr "Hitrost osveževanja:"

#: ui/OutputPanel.qml:152
#, kde-format
msgid "\"%1\" is the only refresh rate supported by this display."
msgstr "»%1« je edina hitrost osveževanja, ki jo podpira ta zaslon."

