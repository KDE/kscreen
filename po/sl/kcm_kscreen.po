#: ui/RotationButton.qml:64
#, kde-format
msgid "No Rotation"
msgstr "Brez obračanja"

#: ui/ScreenView.qml:48
#, kde-format
msgid "Drag screens to re-arrange them"
msgstr "Povlecite zaslone, da jih postavite na mesta"

#: ui/ScreenView.qml:61
#, kde-format
msgid "Identify"
msgstr "Identificiraj"

