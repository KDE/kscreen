msgid "%1%"
msgstr "%%1"

#: ui/OutputPanel.qml:128
#, kde-format
msgid "Refresh rate:"
msgstr "Yenileme hızı:"

#: ui/OutputPanel.qml:148
#, kde-format
msgid "\"%1\" is the only refresh rate supported by this display."
msgstr "Bu görüntü, yalnızca ‘%1’ yenileme hızını destekliyor."

#: ui/OutputPanel.qml:153
#, kde-format
msgid "Adaptive sync:"
msgstr "Uyarlanır eşzamanlama:"

