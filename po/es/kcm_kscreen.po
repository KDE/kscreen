msgid "%1%"
msgstr "%1 %"

#: ui/OutputPanel.qml:372
#, kde-format
msgctxt "@info:tooltip"
msgid "Increases the intensity of non-HDR content on the screen"
msgstr "Aumenta la intensidad del contenido no HDR de la pantalla"

